{
    "name": "demroos/api-gateway-bundle",
    "type": "symfony-bundle",
    "description": "This bundle will help you make api gateway",
    "keywords": ["symfony", "bundle", "api-gateway"],
    "homepage": "https://github.com/demroos/api-gateway-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeniy Kiselev",
            "email": "demroos@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.6",
        "ext-json": "*",
        "symfony/framework-bundle": "^4.3 || ^5.0",
        "guzzlehttp/guzzle": "^6.0",
        "psr/http-message": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.8"
    },
    "autoload": {
        "psr-4": {
            "Demroos\\Bundle\\ApiGatewayBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Demroos\\Bundle\\ApiGatewayBundle\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
